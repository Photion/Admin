    "@nuxtjs/vuetify": "^1.11.2",
    "@types/jest": "^26.0.13",
    "@types/jsdom": "^16.2.3",
    "@types/sinon": "^9.0.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@vue/test-utils": "^1.0.3",
    "cypress": "^4.10.0",
    "eslint": "^7.7.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-nuxt": "^1.0.0",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "sinon": "^9.0.3",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
