{
  "name": "photion-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "expose": "nuxt build && nuxt generate",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test:cypress": "cypress run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "publish:netlify": "yarn nuxt generate && echo '/* / 200' > dist/_redirects",
    "test:integration": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha tests/integration/**/*.test.ts",
    "test:unit": "jest --selectProjects unit"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/composition-api": "^0.12.1",
    "@vue/composition-api": "^1.0.0-beta.10",
    "add": "^2.0.6",
    "aws-sdk": "^2.738.0",
    "axios": "^0.19.2",
    "keylocal": "^0.2.0",
    "moment": "^2.27.0",
    "nuxt": "latest",
    "openpgp": "^4.10.4",
    "uuid": "^8.2.0",
    "vuex-map-fields": "^1.4.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.4",
    "@nuxt/types": "^2.14.1",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
